<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.northlordv2">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.Northlord"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeFeature.CarFeature.CarIndoActivity"
            android:label="@string/title_activity_car_indo"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".ProfileFeature.EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeFeature.CarAddFeature.CarAddActivity"
            android:label="@string/title_activity_car_add"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".MainActivityFeature.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".LoginActivityFeature.SignUpActivity"
            android:label="@string/title_activity_sign_up"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".LoginActivityFeature.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>